<h1 id="header">Create a new account</h1>

<form action="<%= users_url %>" method="POST">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>" >
    <label id="logged" for="email">Email:
        <input id="logged" type="text" name="user[email]" value="<%= @user.email %>">
    </label>
    <br>

    <label id="logged" for="password">Password:
        <input id="logged" type="password" name="user[password]" value="<%= @user.password %>">
    </label>
    <br>
    <label id="button">
        <input type="submit" value="Sign Up!">
    </label>
</form>

